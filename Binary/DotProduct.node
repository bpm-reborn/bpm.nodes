description: "Calculates the dot product of two vectors"
group: Binary
name: DotProduct
extends: Binary/Binary

edges:
  left:
    type: vector
  right:
    type: vector
  result:
    type: number

source: |
  ${OUTPUT.result = 
    (left.x or 0) * (right.x or 0) +
    (left.y or 0) * (right.y or 0) +
    (left.z or 0) * (right.z or 0) +
    (left.w or 0) * (right.w or 0)
  }
