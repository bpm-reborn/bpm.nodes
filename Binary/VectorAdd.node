description: "Adds two vectors"
group: Binary
name: VectorAdd
extends: Binary/Binary

edges:
  left:
    type: vector
  right:
    type: vector
  result:
    type: vector

source: |
  ${OUTPUT.result = {
    x = (left.x or 0) + (right.x or 0),
    y = (left.y or 0) + (right.y or 0),
    z = (left.z or 0) + (right.z or 0),
    w = (left.w or 0) + (right.w or 0)
  }}
