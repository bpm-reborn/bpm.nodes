description: "Calculates the absolute value of a number"
group: Math
name: Abs
extends: Math/Math

edges:
  number:
    description: 'The number to calculate the absolute value of'
    direction: input
    type: number
  result:
    description: 'The absolute value of the number'
    direction: output
    type: number

source: |
  ${OUTPUT.result = math.abs(number)}
  
