description: "Calculates the square root of a number"
group: Math
name: Sqrt
extends: Math/Math

edges:
  number:
    description: 'The number to calculate the square root of'
    direction: input
    type: number
  result:
    description: 'The square root of the number'
    direction: output
    type: number

source: |
  ${OUTPUT.result = math.sqrt(number)}

