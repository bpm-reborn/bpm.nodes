description: "Calculates the tangent of an angle"
group: Math
name: Tan
extends: Math/Math

edges:
  angle:
    description: 'Input angle in radians'
    direction: input
    type: number
  result:
    description: 'Sine of the angle'
    direction: output
    type: number

source: |
  ${OUTPUT.result = math.tan(angle)}
