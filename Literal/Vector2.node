description: "Represents a 2D vector literal"
group: Literal
name: Vector2
extends: Literal/Vector

edges:
  x:
    description: 'X component'
    direction: input
    type: number
    value:
      type: float
      default: 0
  y:
    description: 'Y component'
    direction: input
    type: number
    value:
      type: float
      default: 0
  value:
    description: "Vector2 value"
    direction: output
    type: vector2 or vector
    
source: |
  ${OUTPUT.value = {x = x, y = y}}
