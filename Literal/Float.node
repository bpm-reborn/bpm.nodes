description: "Represents a float literal"
group: Literal
name: Float
extends: Literal/Literal

edges:
  input:
    description: "Float input"
    type: number
    direction: input
    value:
      type: float
      default: 0.0
  value:
    description: "Float value"
    type: number
    direction: output

source: |
  ${OUTPUT.value = input}
